XF86AudioRaiseVolume
	pactl set-sink-volume @DEFAULT_SINK@ +1%; kill -44 $(pidof dwmblocks)
XF86AudioLowerVolume
	pactl set-sink-volume @DEFAULT_SINK@ -1%; kill -44 $(pidof dwmblocks)
XF86AudioMute
	pactl set-sink-mute @DEFAULT_SINK@ toggle; kill -44 $(pidof dwmblocks)
XF86MonBrightnessUp
	light -A 5
XF86MonBrightnessDown
	light -U 5
super + slash
	fishmenu
XF86Tools
	fishmenu
Print
	printscreen
Print + shift
	printscreen -s
